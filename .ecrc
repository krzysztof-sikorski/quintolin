{
  "Version": "2.7.2",
  "Verbose": true,
  "Debug": false,
  "IgnoreDefaults": false,
  "SpacesAftertabs": false,
  "NoColor": false,
  "Exclude": [
    "^.git/",
    "^.idea/",
    "^.php-cs-fixer.cache$",
    "^doc/wayback_machine/",
    "^legacy/",
    "^website/assets/vendor/",
    "^website/public/assets/",
    "^website/public/bundles/",
    "^website/var/",
    "^website/vendor/"
  ],
  "AllowedContentTypes": [],
  "PassedFiles": [],
  "Disable": {
    "EndOfLine": false,
    "Indentation": false,
    "InsertFinalNewline": false,
    "TrimTrailingWhitespace": false,
    "IndentSize": false,
    "MaxLineLength": false
  }
}
